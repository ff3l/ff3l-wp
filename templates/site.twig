{% extends "templates/base.twig" %}

{% block content %}
<div class="banner gallery items-3 autoplay">
  {% for item in menu.get_items %}
  <figure class="item {{ item.classes | join(' ') }}">
    <img src="{{item.get_link}}" alt="{{item.title}}" />
  </figure>
  {% endfor %}
</div>
{% for post in posts %}
  <div class="post">
    {{ post.content }}
  </div>
{% endfor %}
{% endblock %}